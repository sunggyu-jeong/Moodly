{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@processes/*": [
        "src/processes/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@widgets/*": [
        "src/widgets/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@entities/*": [
        "src/entities/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    }
  },
  "include": [
    "src",
    "nativewind-env.d.ts",
    ".env.d.ts",
    "react-native-textinput-extensions.d.ts",
    "hot-updater.config.ts"
  ]
}